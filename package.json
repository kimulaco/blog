{
  "name": "kimulaco-blog",
  "type": "module",
  "private": true,
  "version": "2.0.10",
  "engines": {
    "node": "22.11.0"
  },
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "prepare": "husky",
    "start": "astro dev",
    "dev": "BUILD_ENV=development astro dev",
    "dev:draft": "BUILD_ENV=development DRAFT=true astro dev",
    "build": "astro check && astro build",
    "build:dev": "BUILD_ENV=development pnpm run build",
    "build:prod": "BUILD_ENV=production pnpm run build",
    "preview": "astro preview",
    "astro": "astro",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint:format": "prettier --check '**/*.{js,ts,astro,json,md}'",
    "lint:script": "eslint .",
    "lint:style": "stylelint './src/**/*.{css,scss,astro}' --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/partytown": "^2.0.3",
    "@astrojs/rss": "^4.0.1",
    "@astrojs/sitemap": "^3.0.4",
    "@astrojs/vue": "^4.0.6",
    "@sentry/astro": "^8.36.0",
    "astro": "^4.0.8",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "normalize.css": "^8.0.1",
    "typescript": "^5.3.3",
    "vue": "^3.4.3"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@playwright/test": "^1.40.1",
    "@types/jsdom": "^21.1.6",
    "@types/prismjs": "^1.26.3",
    "@typescript-eslint/eslint-plugin": "8.12.2",
    "@typescript-eslint/parser": "8.12.2",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "2.1.4",
    "@vue/test-utils": "^2.4.3",
    "dotenv": "^16.4.1",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "1.3.0",
    "globals": "15.11.0",
    "husky": "9.1.6",
    "jsdom": "^23.0.1",
    "marked": "^11.1.0",
    "microcms-js-sdk": "^2.7.0",
    "postcss-html": "1.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prismjs": "^1.29.0",
    "sass": "^1.69.6",
    "stylelint": "16.10.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "6.8.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "2.1.4",
    "zx": "8.2.0"
  }
}
