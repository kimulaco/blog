{
  "name": "kimulaco-blog",
  "type": "module",
  "private": true,
  "version": "2.0.7",
  "engines": {
    "node": "20.9.0"
  },
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "prepare": "husky install",
    "start": "astro dev",
    "dev": "BUILD_ENV=development astro dev",
    "dev:draft": "BUILD_ENV=development DRAFT=true astro dev",
    "build": "astro check && astro build",
    "build:dev": "BUILD_ENV=development pnpm run build",
    "build:prod": "BUILD_ENV=production pnpm run build",
    "preview": "astro preview",
    "astro": "astro",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint:format": "prettier --check --ignore-path .gitignore '**/*.{js,ts,astro,json,md}'",
    "lint:script": "eslint --ext .ts,.js,.cjs,.astro --ignore-path .gitignore .",
    "lint:style": "stylelint './src/**/*.{css,scss,astro}' --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/partytown": "^2.0.2",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/vue": "^3.0.4",
    "@sentry/astro": "^7.80.1",
    "astro": "^3.5.4",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "eslint-plugin-astro": "^0.31.0",
    "normalize.css": "^8.0.1",
    "typescript": "^5.2.2",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@types/prismjs": "^1.26.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "marked": "^10.0.0",
    "microcms-js-sdk": "^2.7.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.3",
    "prismjs": "^1.29.0",
    "sass": "^1.69.5",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.0.0",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.6",
    "zx": "^7.2.3"
  }
}
