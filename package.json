{
  "name": "kimulaco-blog",
  "type": "module",
  "version": "2.0.0",
  "engines": {
    "node": "18.*"
  },
  "packageManager": "pnpm@8.10.2",
  "scripts": {
    "prepare": "husky install",
    "start": "astro dev",
    "dev": "BUILD_ENV=development astro dev",
    "build": "astro check && astro build",
    "build:dev": "BUILD_ENV=development pnpm run build",
    "build:prod": "BUILD_ENV=production pnpm run build",
    "preview": "astro preview",
    "astro": "astro",
    "lint:format": "prettier --check --ignore-path .gitignore '**/*.{js,ts,astro,json,md}'",
    "lint:script": "eslint --ext .ts,.js,.cjs,.astro --ignore-path .gitignore .",
    "lint:style": "stylelint './src/**/*.{css,scss,astro}' --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/partytown": "^2.0.2",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/vue": "^3.0.2",
    "@sentry/astro": "^7.80.0",
    "astro": "^3.4.0",
    "date-fns": "^2.30.0",
    "normalize.css": "^8.0.1",
    "typescript": "^5.2.2",
    "vue": "^3.2.30"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "husky": "^8.0.3",
    "marked": "^9.1.5",
    "microcms-js-sdk": "^2.7.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.1",
    "prismjs": "^1.29.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.3.0",
    "vite-svg-loader": "^4.0.0"
  }
}
