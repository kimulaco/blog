{
  "name": "kimulaco-blog",
  "type": "module",
  "private": true,
  "version": "2.1.0",
  "engines": {
    "node": "22.11.0"
  },
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "prepare": "husky",
    "start": "astro dev",
    "dev": "BUILD_ENV=development astro dev",
    "dev:draft": "BUILD_ENV=development DRAFT=true astro dev",
    "build": "astro check && astro build",
    "build:dev": "BUILD_ENV=development pnpm run build",
    "build:prod": "BUILD_ENV=production pnpm run build",
    "preview": "astro preview",
    "astro": "astro",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint:format": "prettier --check '**/*.{js,ts,astro,json,md}'",
    "lint:script": "eslint .",
    "lint:style": "stylelint './src/**/*.{css,scss,astro}' --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/partytown": "2.1.2",
    "@astrojs/rss": "4.0.10",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/vue": "4.5.3",
    "@sentry/astro": "8.41.0",
    "astro": "4.4.0",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "normalize.css": "8.0.1",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@playwright/test": "1.49.0",
    "@types/jsdom": "21.1.7",
    "@types/prismjs": "1.26.5",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vitest/coverage-v8": "2.1.6",
    "@vue/test-utils": "2.4.6",
    "dotenv": "16.4.7",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "1.3.1",
    "globals": "15.12.0",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "marked": "14.1.4",
    "microcms-js-sdk": "3.1.2",
    "postcss-html": "1.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prismjs": "1.29.0",
    "sass": "1.80.6",
    "stylelint": "16.10.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "6.8.1",
    "typescript": "5.6.3",
    "vite-svg-loader": "5.1.0",
    "vitest": "2.1.6",
    "zx": "8.2.4"
  }
}
